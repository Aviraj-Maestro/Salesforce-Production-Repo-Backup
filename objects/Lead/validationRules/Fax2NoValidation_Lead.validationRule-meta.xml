<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fax2NoValidation_Lead</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK( Fax_c__c  )),NOT(REGEX( Fax_c__c , &quot;(\\d){10}&quot;) )
)</errorConditionFormula>
    <errorDisplayField>Fax_c__c</errorDisplayField>
    <errorMessage>Please enter 10 digit Fax.</errorMessage>
</ValidationRule>
