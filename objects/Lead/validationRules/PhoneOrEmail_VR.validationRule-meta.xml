<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PhoneOrEmail_VR</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISBLANK( Phone ) , ISBLANK( Email ))</errorConditionFormula>
