<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>When_Status_is_Draft_Syncing_is_False</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL( Status , &quot;Draft&quot;) ,  IsSyncing = TRUE)</errorConditionFormula>
    <errorDisplayField>IsSyncing</errorDisplayField>
    <errorMessage>Quote Can&apos;t be sync on Draft Status</errorMessage>
</ValidationRule>
