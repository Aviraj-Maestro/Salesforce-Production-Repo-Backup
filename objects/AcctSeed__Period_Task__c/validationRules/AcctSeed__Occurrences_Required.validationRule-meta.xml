<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AcctSeed__Occurrences_Required</fullName>
    <active>true</active>
    <errorConditionFormula>IF(AND(ISPICKVAL(AcctSeed__Ends_On__c, &apos;Number of Occurrences&apos;), ISNULL(AcctSeed__Number_of_Occurrences__c)), true, false)</errorConditionFormula>
