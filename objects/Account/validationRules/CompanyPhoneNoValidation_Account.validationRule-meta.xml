<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CompanyPhoneNoValidation_Account</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK( Alternate_Phone__c)),NOT(REGEX( Alternate_Phone__c, &quot;(\\d){10}&quot;) )
)</errorConditionFormula>
    <errorDisplayField>Alternate_Phone__c</errorDisplayField>
    <errorMessage>Please enter 10 digit Company&apos;s Phone Number.</errorMessage>
</ValidationRule>
