<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Factory_Confirmation_in_case_of_Cancella</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(  Factory_Confirmation_in_case_of_Cancella__c  , &quot;Yes&quot;) ,   NOT(ISPICKVAL(    AcctSeedERP__Status__c , &quot;Closed(Delivered / Cancelled)&quot;) ))</errorConditionFormula>
    <errorDisplayField>AcctSeedERP__Status__c</errorDisplayField>
    <errorMessage>Please select Closed Cancelled</errorMessage>
</ValidationRule>
