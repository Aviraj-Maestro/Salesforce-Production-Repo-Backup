<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AlternatePhoneNoValidation_Contact</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
NOT(ISBLANK( Alternate_No__c)),NOT(REGEX( Alternate_No__c, &quot;(\\d){10}&quot;) )
)</errorConditionFormula>
    <errorDisplayField>Alternate_No__c</errorDisplayField>
    <errorMessage>Please enter 10 digit Alternate Number</errorMessage>
</ValidationRule>
